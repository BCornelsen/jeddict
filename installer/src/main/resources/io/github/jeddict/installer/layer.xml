<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<!--This layer.xml work in Jeddict combined release "io.github.jeddict" , otherwsie move to "io.github.jeddict.installer"--> 
<filesystem>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="io.github.jeddict.instance">
                <attr name="displayName" bundlevalue="io.github.jeddict.installer.update.version.Bundle#Services/AutoupdateType/io.github.jeddict.instance"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="io.github.jeddict.installer.update.version.Bundle#io.github.jeddict"/>
            </file>
        </folder>
    </folder>
</filesystem>